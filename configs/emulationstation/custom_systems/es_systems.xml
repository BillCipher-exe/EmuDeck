<?xml version="1.0"?>
<systemList>
    <system>
        <name>wiiu</name>
        <fullname>Nintendo Wii U</fullname>
        <path>%ROMPATH%/wiiu/roms</path>
        <extension>.rpx .RPX .wud .WUD .wux .WUX .elf .ELF .iso .ISO .wad .WAD .wua .WUA</extension>
        <command label="Cemu (Proton)">/usr/bin/bash /run/media/mmcblk0p1/Emulation/tools/launchers/cemu.sh -f -g z:%ROM%</command>
        <platform>wiiu</platform>
        <theme>wiiu</theme>
    </system>
    <system>
        <name>arcade</name>
        <fullname>Arcade</fullname>
        <path>%ROMPATH%/mame/roms</path>
        <extension>.cmd .CMD .7z .7Z .zip .ZIP</extension>
        <command label="MAME (Proton)">/usr/bin/bash /run/media/mmcblk0p1/Emulation/tools/launchers/mame-proton.sh -rompath %ROMPATH% %BASENAME%</command>
        <platform>arcade</platform>
        <theme>arcade</theme>
    </system>
</systemList>
